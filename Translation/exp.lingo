assignment = " "* letter+ $w " "*"="" "* exp:val " "*";" {assign($w, :val)};
exp = sum|sub|prod|div|int|var;
sum = " "*"("" "* exp:l " "*"+"" "* exp:r " "*")"" "* {sumOp(:l, :r)};
sub = " "*"("" "* exp:l " "*"-"" "* exp:r " "*")"" "* {subOp(:l, :r)};
prod = " "*"("" "* exp:l " "*"*"" "* exp:r " "*")"" "* {prodOp(:l, :r)};
div = " "*"("" "* exp:l " "*"/"" "* exp:r " "*")"" "* {divOp(:l, :r)};
digit = '0'-'9';
int = digit+ $i {ArInt(s2i($i))};
letter = 'a'-'z';
var = letter+ $v {variable($v)};
